{
    "manifest_version":3,
    "name":"GeM Invoice Upload",
    "version":"0.01",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["content.js"]
        }
    ],
    "permissions":["activeTab", "tabs"],
    "action":{
        "default_icon":"gem.png",
        "default_popup": "popup.html",
        "default_title":"GeM Invoice Upload"
    }
}