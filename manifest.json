{
    "manifest_version":3,
    "name":"GeM Invoice Upload",
    "version":"1.0",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["content.js"]
        }
    ],
    "icons":{"36":"gem36.png","48":"gem48.png"},
    "permissions":["activeTab", "tabs"],
    "action":{
        "default_popup": "popup.html",
        "default_title":"GeM Invoice Upload"
    }
}